public class Student {  
  
int b;  
String name;  
  
Student(){  
System.out.println("this a default constructor");  
b=1;
name="Ram";
}  
  
Student(int i, String n){  
b = i;  
name = n;  
}  
  
public static void main(String[] args) {  
 
Student s = new Student();  
System.out.println("\nDefault Constructor called: \n");  
System.out.println("Student Id : "+s.b + "\nStudent Name : "+s.name);  
  
System.out.println("\nParameterized Constructor values: \n");  
Student student = new Student(10, "Ayush");  
System.out.println("Student Id : "+student.b + "\nStudent Name : "+student.name);  
}  
} public class Student {  
  
int b;  
String name;  
  
Student(){  
System.out.println("this a default constructor");  
b=1;
name="Ram";
}  
  
Student(int i, String n){  
b = i;  
name = n;  
}  
  
public static void main(String[] args) {  
 
Student s = new Student();  
System.out.println("\nDefault Constructor called: \n");  
System.out.println("Student Id : "+s.b + "\nStudent Name : "+s.name);  
  
System.out.println("\nParameterized Constructor values: \n");  
Student student = new Student(10, "Ayush");  
System.out.println("Student Id : "+student.b + "\nStudent Name : "+student.name);  
}  
} 
